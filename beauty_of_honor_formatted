javascript:var honors=document.querySelector(".txt-total-record").childNodes[0].innerHTML.replaceAll(',',''),objective=parseInt(prompt("Please enter your honor goal", "0")),d_honors=objective-honors;var nm_honors={"NM250": 50000000,"NM200": 14000000,"NM150": 4500000,"NM100": 3000000,"NM95": 1000000,"NM90": 350000,"EX+": 130000,"EX": 70000,"VH": 25000,"Hard": 8000,"Normal": 4000};var message = "Fights breakdown :\n";for (const [key, value] of Object.entries(nm_honors)){var fights=Math.floor(d_honors/value);d_honors-=fights*value;if (fights != 0){message+=key + ": " + fights + "\n";}}message+="Remainder: " + d_honors;alert(message)